{
  "type": "record",
  "name": "MCPGatewayConfig",
  "fields": [
    {
      "name": "broker_suggested_server_name",
      "type": {
        "type": "record",
        "name": "BrokerSuggServerNameConfig",
        "fields": [
          {
            "name": "enable",
            "type": "boolean",
            "default": true,
            "$ui": {
              "component": "switch",
              "flex": 12,
              "required": false,
              "label": "$enable_broker_suggested_server_name"
            }
          },
          {
            "name": "load_file",
            "type": "string",
            "$ui": {
              "component": "input",
              "flex": 24,
              "required": false,
              "label": "$load_file_s"
            }
          },
          {
            "name": "deny_if_no_match",
            "type": "boolean",
            "default": true,
            "$ui": {
              "component": "switch",
              "flex": 12,
              "required": false,
              "label": "$deny_if_no_match"
            }
          }
        ]
      }
    },
    {
      "name": "broker_suggested_server_name_filters",
      "type": {
        "type": "record",
        "name": "BrokerSuggServerNameFiltersConfig",
        "fields": [
          {
            "name": "enable",
            "type": "boolean",
            "default": true,
            "$ui": {
              "component": "switch",
              "flex": 12,
              "required": false,
              "label": "$enable_broker_suggested_server_name_filters_for_clients"
            }
          },
          {
            "name": "load_file",
            "type": "string",
            "$ui": {
              "component": "input",
              "flex": 24,
              "required": false,
              "label": "$load_file_c"
            }
          },
          {
            "name": "deny_if_no_match",
            "type": "boolean",
            "default": true,
            "$ui": {
              "component": "switch",
              "flex": 12,
              "required": false,
              "label": "$deny_if_no_match"
            }
          }
        ]
      }
    },
    {
      "name": "mcp_client_rbac",
      "type": {
        "type": "record",
        "name": "MCPClientsRBACConfig",
        "fields": [
          {
            "name": "enable",
            "type": "boolean",
            "default": true,
            "$ui": {
              "component": "switch",
              "flex": 12,
              "required": false,
              "label": "$enable_mcp_client_rbac"
            }
          },
          {
            "name": "load_file",
            "type": "string",
            "$ui": {
              "component": "input",
              "flex": 24,
              "required": false,
              "label": "$load_file_client_rbac"
            }
          }
        ]
      }
    },
    {
      "name": "stdio_servers",
      "type": {
        "type": "map",
        "values": {
          "type": "record",
          "name": "ServerConfig",
          "fields": [
            {
              "name": "enable",
              "type": "boolean",
              "default": true,
              "$ui": {
                "component": "switch",
                "flex": 12,
                "required": false,
                "label": "$enable"
              }
            },
            {
              "name": "server_name",
              "type": "string",
              "$ui": {
                "component": "input",
                "flex": 12,
                "required": true,
                "label": "$server_name"
              }
            },
            {
              "name": "server_desc",
              "type": "string",
              "$ui": {
                "component": "input",
                "flex": 12,
                "required": false,
                "label": "$server_desc"
              }
            },
            {
              "name": "command",
              "type": "string",
              "$ui": {
                "component": "input",
                "flex": 24,
                "required": true,
                "label": "$command"
              }
            },
            {
              "name": "args",
              "type": {
                "type": "array",
                "items": "string"
              },
              "default": [],
              "$ui": {
                "component": "input-array",
                "flex": 12,
                "required": false,
                "label": "$args"
              }
            },
            {
              "name": "env",
              "type": {
                "type": "map",
                "values": "string"
              },
              "default": {},
              "$ui": {
                "component": "key-value-editor",
                "flex": 12,
                "required": false,
                "label": "$env"
              }
            }
          ]
        }
      }
    }
  ]
}
